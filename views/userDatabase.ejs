<!DOCTYPE html>
<html>
<head>
    <title>User Database</title>
</head>
<body>
    <h1>All Users</h1>
    <table border="1">
        <thead>
            <tr>
                <th>User ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Mission Call</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
            <tr>
                <td><%= user.user_id %></td>
                <td><%= user.first_name %></td>
                <td><%= user.last_name %></td>
                <td><%= user.mission_call %></td>
                <td><a href="/users/<%= user.user_id %>">View</a></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
